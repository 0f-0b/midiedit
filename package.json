{
  "name": "midiedit",
  "productName": "MIDIEdit",
  "version": "0.0.0",
  "description": "Yet another MIDI editor.",
  "keywords": [
    "midi"
  ],
  "private": true,
  "homepage": "https://github.com/sjx233/midiedit#readme",
  "bugs": "https://github.com/sjx233/midiedit/issues",
  "license": "MIT",
  "author": "sjx233",
  "main": "build/main.js",
  "repository": "github:sjx233/midiedit",
  "scripts": {
    "lint": "eslint './src/**/*' './types/**/*'",
    "check": "tsc",
    "build": "node scripts/build.js",
    "build:prod": "cross-env NODE_ENV=production npm run build",
    "start": "electron .",
    "package": "npm run build:prod && node scripts/package.js",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "midievents": "^2.0.0",
    "midifile": "^2.0.0",
    "react": "^17.0.1",
    "react-beforeunload": "^2.4.0",
    "react-dom": "^17.0.1",
    "react-virtualized": "^9.22.2",
    "use-latest": "^1.2.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.12",
    "@types/react": "^17.0.0",
    "@types/react-beforeunload": "^2.1.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-virtualized": "^9.21.10",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "cross-env": "^7.0.3",
    "csso": "^4.2.0",
    "electron": "^11.1.0",
    "electron-packager": "^15.2.0",
    "esbuild": "^0.8.21",
    "eslint": "^7.15.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-minifier": "^4.0.0",
    "typescript": "^4.1.3"
  }
}
